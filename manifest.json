{
  "manifest_version": 2,
  "name": "skrrt",
  "description": "The new tab that makes everything faster.",
  "version": "0.1",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage", "topSites", "tabs", "commands", "https://*/", "http://*/"],
  "commands": {
    "open-first-site": {
      "suggested_key": {
        "default": "Alt+1",
        "mac": "Alt+1"
       },
      "description": "Open the most used site"
    },    
    "open-second-site": {
      "suggested_key": {
        "default": "Alt+2",
        "mac": "Alt+2"
       },
      "description": "Open the second most used site"
    },
    "open-third-site": {
      "suggested_key": {
        "default": "Alt+3",
        "mac": "Alt+3"
       },
      "description": "Open the third most used site"
    },
    "open-settings": {
      "suggested_key": {
        "default": "Alt+H",
        "mac": "Alt+H"
      },
      "description": "Open settings menu"
    },
    "toggle-minimalist": {
      "description": "Toggle minimalist"
    }
  }
}
